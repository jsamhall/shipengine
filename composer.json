{
    "name": "jsamhall/shipengine",
    "description": "A PHP wrapper for the accessing and interacting with the ShipEngine v1 API.",
    "license": "MIT",
    "keywords": [
        "shipengine",
        "shipstation"
    ],
    "authors": [
        {
            "name": "John Hall",
            "email": "john@johnhall.io"
        }
    ],
    "require": {
        "php": "^7.2.0",
        "ext-curl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.5|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "jsamhall\\ShipEngine\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    }
}
